<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Titre</th>
      <th>Description</th>
      <th>Contenu</th>
      <th>Type</th>
      <th>Telechagement</th>
      <th>Vue</th>
      <th>Like</th>
      <th>Admin users</th>
      <th>Users</th>
      <th>Album</th>
      <th>Mixtape</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.titre %></td>
        <td><%= post.description %></td>
        <td><%= post.contenu %></td>
        <td><%= post.type %></td>
        <td><%= post.telechagement %></td>
        <td><%= post.vue %></td>
        <td><%= post.like %></td>
        <td><%= post.admin_users %></td>
        <td><%= post.users %></td>
        <td><%= post.album %></td>
        <td><%= post.mixtape %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
